<!--components/swiperBox/swiperBox.wxml-->
<view class="">
  <swiper class="swiper" autoplay indicator-dots circular>
    <block 
    wx:for="{{swiperList}}" 
    wx:key="goods_id">
      <swiper-item>
        <image src="{{item.image_src}}" mode="widthFix" />
      </swiper-item>
    </block>
  </swiper>
</view>
